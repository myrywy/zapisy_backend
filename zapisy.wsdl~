			</xs:restriction>
		</xs:simpleType>
		<xs:simpleType name="wynik">
			<xs:restriction base="xs:string">
				<xs:pattern value="ok|blad"/>
			</xs:restriction>
		</xs:simpleType>
		<xs:complexType name="student">
			<xs:all>
